<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <schedule>

    <job>
      <name>ReportingJob</name>
      <group>Replication</group>
      <description>ReportingJob</description>
      <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.ReportingJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <simple>
        <name>Запуск ReportingJob через simpletrigger</name>
        <group>Replication</group>

        <job-name>ReportingJob</job-name>
        <job-group>Replication</job-group>

        <repeat-count>-1</repeat-count>
        <repeat-interval>5000</repeat-interval>
      </simple>
    </trigger>
    
    <!-- ==================================================================================================================================== -->
	  <job>
		  <name>OperationsPrimaryProcessingJob</name>
		  <group>CustomerIntelligence</group>
		  <description>ImportFactsFromErmFlow</description>
		  <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.OperationsPrimaryProcessingJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

		  <durable>true</durable>
		  <recover>false</recover>
	  </job>

	  <trigger>
		  <cron>
			  <name>ImportFactsFromErmFlow</name>
			  <group>CustomerIntelligence</group>

			  <job-name>OperationsPrimaryProcessingJob</job-name>
			  <job-group>CustomerIntelligence</job-group>

			  <job-data-map>
				  <entry>
					  <key>BatchSize</key>
					  <value>25</value>
				  </entry>
				  <entry>
					  <key>Flow</key>
					  <value>ImportFactsFromErmFlow</value>
				  </entry>
			  </job-data-map>

			  <cron-expression>0/1 * * * * ?</cron-expression>

		  </cron>
	  </trigger>

	  <trigger>
		  <cron>
			  <name>ImportFactsFromBitFlow</name>
			  <group>CustomerIntelligence</group>

			  <job-name>OperationsPrimaryProcessingJob</job-name>
			  <job-group>CustomerIntelligence</job-group>

			  <job-data-map>
				  <entry>
					  <key>BatchSize</key>
					  <value>1</value>
				  </entry>
				  <entry>
					  <key>Flow</key>
					  <value>ImportFactsFromBitFlow</value>
				  </entry>
			  </job-data-map>

			  <cron-expression>0 0/1 * * * ?</cron-expression>

		  </cron>
	  </trigger>

    <!-- ==================================================================================================================================== -->
    <job>
      <name>OperationsFinalProcessingJob</name>
      <group>CustomerIntelligence</group>
      <description>OperationsFinalProcessingJob</description>
      <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.OperationsFinalProcessingJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>ImportAggregatesFlow</name>
        <group>CustomerIntelligence</group>

        <job-name>OperationsFinalProcessingJob</job-name>
        <job-group>CustomerIntelligence</job-group>

        <job-data-map>
          <entry>
            <key>BatchSize</key>
            <value>5000</value>
          </entry>
          <entry>
            <key>Flows</key>
            <value>AggregatesFlow</value>
          </entry>
        </job-data-map>

		<cron-expression>0/2 * * * * ?</cron-expression>

	  </cron>
    </trigger>

  </schedule>
</job-scheduling-data>
