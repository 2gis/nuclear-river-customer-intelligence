	  <add name="Dest" connectionString="Endpoint=sb://uk-sql02.2gis.local/ERM-Test{EnvNum},sb://uk-sql01.2gis.local/ERM-Test{EnvNum};StsEndpoint=https://uk-sql02.2gis.local:9355/ERM-Test{EnvNum},https://uk-sql01.2gis.local:9355/ERM-Test{EnvNum};RuntimePort=9354;ManagementPort=9355"
		   xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
