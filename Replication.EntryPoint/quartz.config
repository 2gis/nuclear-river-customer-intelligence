    <!-- ==================================================================================================================================== -->
    <job>
      <name>OperationsPrimaryProcessingJob</name>
      <group>Interruptable</group>
      <description>OperationsPrimaryProcessingJob</description>
      <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.OperationsPrimaryProcessingJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <simple>
        <name>PrimaryReplicate2CustomerIntelligenceFlow</name>
        <group>CustomerIntelligence</group>

        <job-name>OperationsPrimaryProcessingJob</job-name>
        <job-group>Interruptable</job-group>

        <job-data-map>
          <entry>
            <key>BatchSize</key>
            <value>1000</value>
          </entry>
          <entry>
            <key>Flow</key>
            <value>Replicate2CustomerIntelligenceFlow</value>
          </entry>
          <entry>
            <key>TimeSafetyOffsetHours</key>
            <value>2</value>
          </entry>
        </job-data-map>

        <repeat-count>0</repeat-count>
        <repeat-interval>0</repeat-interval>

      </simple>
    </trigger>

    <!-- ==================================================================================================================================== -->
    <job>
      <name>OperationsFinalProcessingJob</name>
      <group>CustomerIntelligence</group>
      <description>OperationsFinalProcessingJob</description>
      <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.OperationsFinalProcessingJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>BatchSize</key>
          <value>1000</value>
        </entry>
        <entry>
          <key>Flows</key>
          <value>Replicate2CustomerIntelligenceFlow</value>
        </entry>
        <entry>
          <key>ReprocessingBatchSize</key>
          <value>10</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <simple>
        <name>FinalReplicate2CustomerIntelligenceFlow</name>
        <group>CustomerIntelligence</group>

        <job-name>OperationsFinalProcessingJob</job-name>
        <job-group>CustomerIntelligence</job-group>

        <repeat-count>-1</repeat-count>
        <repeat-interval>2000</repeat-interval>
      </simple>
    </trigger>
    
  </schedule>
