<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <schedule>

    <!--Debug Test job -->
    <job>
      <name>DebugTestJob</name>
      <group>Replication</group>
      <description>DebugTestJob</description>
      <job-type>NuClear.AdvancedSearch.Replication.EntryPoint.Jobs.DebugTestJob, 2GIS.NuClear.AdvancedSearch.Replication.EntryPoint</job-type>

      <durable>true</durable>
      <recover>false</recover>

    </job>

    <trigger>
      <simple>
        <name>Запуск DebugTestJob через simpletrigger</name>
        <group>Replication</group>

        <job-name>DebugTestJob</job-name>
        <job-group>Replication</job-group>

        <repeat-count>0</repeat-count>
        <repeat-interval>0</repeat-interval>
      </simple>
    </trigger>
    
  </schedule>
</job-scheduling-data>
